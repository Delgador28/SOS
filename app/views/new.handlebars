<h1 id="create">Get Started</h1>
<div id="createStuff">
   <form class="create-update-form" action="/api/new-user" method="POST">
<div id="inputBoxCreate">
       <div class="form-group">
           <label for="name"></label>
           <input type="text" placeholder="Full Name" id="full-name" name="name">
       </div>
       <div class="form-group">
           <label for="email_add"></label>
           <input type="text" placeholder="Email" id="email" name="email_add">
       </div>
       <div class="form-group">
           <label for="phone_number"></label>
           <input type="number" placeholder="Phone" id="phone" name="phone_number">
       </div>
       <div class="form-group">
           <label for="user_name"></label>
           <input type="text" placeholder="Username" id="username" name="user_name">
       </div>
       <div class="form-group">
           <label for="user_password"></label>
           <input type="password" placeholder="password" id="pass" name="user_password">
       </div>
</div>
</div>
   <button class="btn btn-dark" id="submit" type="submit">Create Account</button>
</form>
</div>